{
  "manifest_version": 2,
  "name": "Open in Private Tab",
  "version": "0.1",
  "background": {
  	"scripts": ["background.js"]
  },
  "content_scripts": [
  	{
  		"matches": [
  			"<all_urls>"
  		],
  		"js": ["jquery-3.1.0.min.js","content.js"]
  	}	
  ],
  "browser_action": {
  		"default_icon": "icon.png"
	},
  	"permissions": [ 
  		"tabs"
	]
}
